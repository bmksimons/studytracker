       <NumberAxis fx:id="naHours" label="Antall Timer" side="LEFT" />
     </yAxis>
   </BarChart>
   <Button fx:id="closeStatistics" layoutX="309.0" layoutY="413.0" mnemonicParsing="false" onAction="#onCloseStatisticsClick" text="Back to home page" />
